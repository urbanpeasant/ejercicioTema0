package introduccion;

public class TiposPrimitivos {

	
	public static void main (String [] args) {
		
		byte minumero = (byte) 55;
		System.out.println(minumero);
		short v2=minumero;
		System.out.println(minumero);
		int v3 = 77;
		
		//convierte entero a byte pero se pierde informaci√≥n
		minumero = (byte)v3;
		System.out.println(minumero);
	}
}
